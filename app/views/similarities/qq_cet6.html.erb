<script type="text/javascript">
  function getHttpParams(name)
  {
    var r = new RegExp("(\\?|#|&)"+name+"=([^&#]*)(&|#|$)");
    var m = location.href.match(r);
    return decodeURIComponent(!m?"":m[2]);
  }
  var openkey=getHttpParams("openkey");
  var openid=getHttpParams("openid");
  var web_type=getHttpParams("web");

  $.ajax({
    async:true,
    dataType:'json',
    type:'post',
    url:"/similarities/check_status",
    data:{
      openid:openid
    },
    success : function(data) {
      if (data){
         document.write("<iframe src='/similarities/request_qq6' style='width:754px;height:628px;border:0px;'></iframe>");
      }else{
        window.location.href="/similarities?web=qq&category=3"
      }
    }
  });
</script>
