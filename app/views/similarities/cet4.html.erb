<body>
  <script type="text/javascript" src="/assets/renren.js"></script>
  <div class="wrap renren_border">
    <div class="main_Div">
      <img style="height:598px;" src="/assets/login_pic4.png"
           onclick='window.location.href = "/similarities?category=<%= Category::LEVEL_FOUR %>&web=renren&appid=<%=@client_id %>";' />
      <div id="hidden_iframe" style="display:none;">
        <% unless params[:xn_sig_added]=="1" %>
          <script type="text/javascript">
            var uiOpts = {
              url : "http://graph.renren.com/oauth/authorize",
              display : "iframe",
              params : {"response_type":"token","client_id":"<%= @client_id %>"},
              onSuccess: function(r){
                window.location = "/similarities?category=<%= Category::LEVEL_FOUR %>&web=renren&appid=<%=@client_id %>"
              }
            };
            var root_flag = 0;
            Renren.ui(uiOpts);
          </script>
        <% end %>
      </div>
    </div>
    <div class="foot">Powered by 赶考网 &copy; 2010-2012</div>
  </div>
</body>