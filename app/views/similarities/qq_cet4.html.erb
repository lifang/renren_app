<script type="text/javascript">
  function getHttpParams(name)
  {
    var r = new RegExp("(\\?|#|&)"+name+"=([^&#]*)(&|#|$)");
    var m = location.href.match(r);
    return decodeURIComponent(!m?"":m[2]);
  }
  var openkey=getHttpParams("openkey");
  var openid=getHttpParams("openid");
  var web_type=getHttpParams("web");

  $.ajax({
    async:true,
    dataType:'json',
    type:'post',
    url:"/similarities/check_status",
    data:{
      openid:openid
    },
    success : function(data) {
      if (data){
        window.open('/similarities/request_qq', '_self', 'height=500,width=600,left=300,top=150');
      }else{
        window.location.href="/similarities?web=qq&category=2"
      }
    }
  });
</script>
